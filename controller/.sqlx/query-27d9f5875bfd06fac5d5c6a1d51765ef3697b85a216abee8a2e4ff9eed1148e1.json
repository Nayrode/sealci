  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                a.id            AS action_id,\n                a.pipeline_id,\n                a.name,\n                a.type          AS action_type,\n                a.container_uri,\n                a.status,\n                c.command       AS \"command?\",\n                c.id            AS \"command_id?\"\n            FROM actions a\n            LEFT JOIN commands c ON a.id = c.action_id\n            WHERE a.id = $1\n            ",
